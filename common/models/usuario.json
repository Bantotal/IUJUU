{
  "name": "usuario",
  "base": "User",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true
    },
    "documento": {
      "type": "string"
    },
    "fechaDeNacimiento": {
      "type": "date"
    },
    "avatar": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "regalos": {
      "type": "hasMany",
      "model": "regalo",
      "foreignKey": "regaloId"
    },
    "participacions": {
      "type": "hasMany",
      "model": "participacion",
      "foreignKey": "participacionId"
    },
    "cuenta": {
      "type": "hasMany",
      "model": "cuenta",
      "foreignKey": "cuentaId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
