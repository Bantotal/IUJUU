{
  "name": "usuario",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "documento": {
      "type": "string"
    },
    "fechaDeNacimiento": {
      "type": "date"
    },
    "avatar": {
      "type": "string"
    },
    "contrasena": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "regalos": {
      "type": "hasMany",
      "model": "regalo",
      "foreignKey": "regaloId"
    },
    "participacions": {
      "type": "hasMany",
      "model": "participacion",
      "foreignKey": "participacionId"
    },
    "cuenta": {
      "type": "hasMany",
      "model": "cuenta",
      "foreignKey": "cuentaId"
    }
  },
  "acls": [],
  "methods": {}
}
